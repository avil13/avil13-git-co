{
  "name": "avil13-co",
  "version": "1.1.3",
  "description": "",
  "main": "dist/index.js",
  "public": true,
  "bin": {
    "co": "./bin/co"
  },
  "files": [
    "dist",
    "bin",
    "src",
    "assets"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node --project tsconfig.json src/index.ts",
