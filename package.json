    "inquirer": "^7.0.4",
    "inquirer-autocomplete-prompt": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
